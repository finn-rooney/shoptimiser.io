---
import Layout from "../layouts/Layout.astro";
import ProtocolStep from "../components/ProtocolStep.astro";
import Brand from "../components/Brand.astro";
import CartDrawingSVG from "../components/svg-components/CartDrawingSVG.astro";
import Footer from "../components/Footer.astro";
import ServiceFAQ from "../components/ServiceFAQ.astro";

const scheduleURL = "/schedule/";
---

<Layout title="Shoptimiser - Capture Future Missed Conversions">
    <!-- Page Specific Styles -->
    <style>
        .hidden-element {
            transition: all 1s ease-in-out;
            opacity: 0;
            transform: rotateZ(0) translateY(200px);
        }
        .show {
            opacity: 1;
            transform: translateY(0);
        }
        .show.rotate:nth-child(1) {
            opacity: 1;
            transform: rotateZ(-10deg) translateY(0);
        }
        .show.rotate:nth-child(2) {
            opacity: 1;
            transform: rotateZ(-40deg) translateY(-40px);
        }
        .show.rotate:nth-child(3) {
            opacity: 1;
            transform: rotateZ(10deg) translateY(0);
        }

        @media (max-width: 600px) {
            .result {
                width: 150px !important;
            }
        }

        @media (max-width: 640px) {
            .container {
                max-width: 512px;
            }
        }

        @media (max-width: 512px) {
            .container {
                max-width: 400px;
            }
        }

        @media (max-width: 400px) {
            .container {
                max-width: 350px;
            }
        }

    </style>
    <!-- Main Start -->
    <section class="min-h-screen bg-slate-50 lg:min-h-screen">
        
        <!-- Nav Start -->
        <div
            class="container mx-auto flex justify-center pt-6 md:items-center md:justify-between md:py-6"
        >
            <img class="w-72" src="/shoptimiser-alt-svg.svg" />
            <ul class="hidden items-center gap-4 text-slate-900 md:flex">
                <li>
                    <button
                        class="uppercase hover:text-slate-700 hover:bg-white transition duration-300 ease-in-out py-3 px-4 rounded-md"
                        onclick="scrollToServiceSection()"
                    >
                        FAQ
                    </button>
                </li>
                <li>
                    <button
                        class="uppercase hover:text-slate-700 hover:bg-white transition duration-300 ease-in-out py-3 px-4 rounded"
                        onclick="scrollToProcessSection()"
                    >
                        What We Do
                    </button>
                </li>
                <li>
                    <a
                        href={scheduleURL}
                        class="rounded border-2 border-green-300 bg-transparent px-4 py-1 font-semibold text-green-300 transition duration-300 ease-in-out hover:border-green-300 hover:bg-green-300 hover:text-white focus:outline-none sm:px-8 sm:py-3 uppercase"
                    >
                        Free Audit
                    </a>

                </li>
            </ul>
        </div>
        <!-- Nav End -->


        <!-- Hero Start -->
        <div class="flex items-center">

            <div class="container mx-auto py-[2vh] lg:px-20">
                <div
                    class="flex flex-col-reverse items-center text-center lg:mt-12 lg:flex-row lg:text-left"
                >
                    <!--Hero Left Start-->
                    <div class="lg:w-7/12">
                        <!--Hero Heading/Subheading Start-->
                        <h1
                            class="text-4xl font-bold leading-none text-gray-700 md:text-5xl"
                        >
                            We
                            <span class="text-green-300">
                                Increase Store Conversions By 25%
                            </span>
                            Without Spending More Money Within 30 Days.
                        </h1>

                            <div class="lg:hidden">
                                <CartDrawingSVG classList="max-h-96" />
                            </div>

